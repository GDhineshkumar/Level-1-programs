# In[14]:


def create_even_submatrix(n):
    matrix = [[0] * n for _ in range(n)]

    for i in range(n):
        for j in range(n):
            if (i + j) % 2 == 0:
                matrix[i][j] = 1

    return matrix


# Test the function
n = 4
result = create_even_submatrix(n)

# Print the resulting matrix
for row in result:
    print(row)


